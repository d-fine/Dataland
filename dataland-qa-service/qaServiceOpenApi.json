  "paths": {
    "/data/sfdr/{dataId}/reports/{qaReportId}/status": {
      "put": {
        "tags": [
          "sfdr-data-qa-report-controller"
        ],
        "summary": "Change the status of a QA report.",
        "description": "The specified QA report is marked as active or inactive.",
        "operationId": "setQaReportStatus",
        "parameters": [
          {
            "name": "dataId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "qaReportId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/QaReportStatusPatch"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successfully marked QA report as active or inactive."
          },
          "default": {
            "description": "An error occurred",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "headers": {
              "WWW-Authenticate": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        },
        "security": [
          {
            "default-oauth": []
          },
          {
            "default-bearer-auth": []
          }
        ]
      }
    },
    "/datasets/{dataId}": {
      "get": {
        "tags": [
          "qa-controller"
        ],
        "summary": "Gets the QA review status of an uploaded dataset for a given id.",
        "description": "Get the QA review status of uploaded dataset for a given id.Users can get the review status of their own datasets.Admins and reviewer can get the review status for all datasets.",
        "operationId": "getDatasetById",
        "parameters": [
          {
            "name": "dataId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Found a QA review status corresponding the id.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ReviewInformationResponse"
                }
              }
            }
          },
          "404": {
            "description": "Found no QA review status corresponding the id.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ReviewInformationResponse"
                }
              }
            }
          },
          "default": {
            "description": "An error occurred",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "headers": {
              "WWW-Authenticate": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        },
        "security": [
          {
            "default-oauth": []
          },
          {
            "default-bearer-auth": []
          }
        ]
      },
      "post": {
        "tags": [
          "qa-controller"
        ],
        "summary": "Assign a quality status to a unreviewed dataset",
        "description": "Set the quality status after a dataset has been reviewed",
        "operationId": "assignQaStatus",
        "parameters": [
          {
            "name": "dataId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "qaStatus",
            "in": "query",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/QaStatus"
            }
          },
          {
            "name": "message",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successfully assigned quality status to dataset."
          },
          "default": {
            "description": "An error occurred",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "headers": {
              "WWW-Authenticate": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        },
        "security": [
          {
            "default-oauth": []
          },
          {
            "default-bearer-auth": []
          }
        ]
      }
    },
    "/data/sfdr/{dataId}/reports": {
      "get": {
        "tags": [
          "sfdr-data-qa-report-controller"
        ],
        "summary": "retrieve all QA reports.",
        "description": "retrieve all QA reports associated to given dataset.",
        "operationId": "getAllQaReportsForDataset",
