          "sustainableOceansAndSeasPolicy": {
            "$ref": "#/components/schemas/QaReportDataPointExtendedDataPointYesNoNoEvidenceFound"
          },
          "threatenedSpeciesExposure": {
            "$ref": "#/components/schemas/QaReportDataPointExtendedDataPointYesNoNoEvidenceFound"
          },
          "biodiversityProtectionPolicy": {
            "$ref": "#/components/schemas/QaReportDataPointExtendedDataPointYesNoNoEvidenceFound"
