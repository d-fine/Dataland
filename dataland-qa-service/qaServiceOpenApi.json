  "components": {
    "schemas": {
      "QaReportStatusPatch": {
        "required": [
          "active"
        ],
        "type": "object",
        "properties": {
          "active": {
            "type": "boolean"
          }
        }
      },
      "QaStatus": {
        "type": "string",
        "enum": [
          "Pending",
          "Accepted",
          "Rejected"
        ]
      },
      "CompanyReport": {
        "required": [
          "fileReference"
        ],
        "type": "object",
        "properties": {
          "fileReference": {
            "type": "string"
          },
          "fileName": {
            "type": "string"
          },
          "publicationDate": {
            "type": "string",
            "format": "date"
          }
        }
      },
      "CurrencyDataPoint": {
        "type": "object",
        "properties": {
          "value": {
            "type": "number"
          },
          "quality": {
            "type": "string",
            "enum": [
              "Audited",
              "Reported",
              "Estimated",
              "Incomplete"
            ]
          },
          "comment": {
            "type": "string"
          },
          "dataSource": {
            "$ref": "#/components/schemas/ExtendedDocumentReference"
          },
          "currency": {
            "type": "string"
          }
        }
      },
      "ExtendedDataPointBigDecimal": {
        "type": "object",
        "properties": {
          "value": {
            "type": "number"
          },
          "quality": {
            "type": "string",
            "enum": [
              "Audited",
              "Reported",
              "Estimated",
              "Incomplete"
            ]
          },
          "comment": {
            "type": "string"
          },
          "dataSource": {
            "$ref": "#/components/schemas/ExtendedDocumentReference"
          }
        }
      },
      "ExtendedDataPointBigInteger": {
        "type": "object",
        "properties": {
          "value": {
            "type": "integer",
            "format": "int32"
          },
          "quality": {
            "type": "string",
            "enum": [
              "Audited",
              "Reported",
              "Estimated",
              "Incomplete"
            ]
          },
          "comment": {
            "type": "string"
          },
          "dataSource": {
            "$ref": "#/components/schemas/ExtendedDocumentReference"
          }
        }
      },
      "ExtendedDataPointYesNoNoEvidenceFound": {
        "type": "object",
        "properties": {
          "value": {
            "type": "string",
            "enum": [
              "Yes",
              "No",
              "NoEvidenceFound"
            ]
          },
          "quality": {
            "type": "string",
            "enum": [
              "Audited",
              "Reported",
              "Estimated",
              "Incomplete"
            ]
          },
          "comment": {
            "type": "string"
          },
          "dataSource": {
            "$ref": "#/components/schemas/ExtendedDocumentReference"
          }
        }
      },
      "ExtendedDocumentReference": {
        "required": [
          "fileReference"
        ],
        "type": "object",
        "properties": {
          "fileReference": {
            "type": "string"
          },
          "page": {
            "type": "integer",
            "format": "int64"
          },
          "tagName": {
            "type": "string"
          },
          "fileName": {
            "type": "string"
          }
        }
      },
      "QaReportDataPointExtendedDataPointBigDecimal": {
        "required": [
          "comment",
          "correctedData",
          "verdict"
        ],
        "type": "object",
        "properties": {
          "comment": {
            "type": "string"
          },
          "verdict": {
            "$ref": "#/components/schemas/QaReportDataPointVerdict"
          },
          "correctedData": {
            "$ref": "#/components/schemas/ExtendedDataPointBigDecimal"
          }
        }
      },
      "QaReportDataPointExtendedDataPointBigInteger": {
        "required": [
          "comment",
          "correctedData",
          "verdict"
        ],
        "type": "object",
        "properties": {
          "comment": {
            "type": "string"
          },
          "verdict": {
            "$ref": "#/components/schemas/QaReportDataPointVerdict"
          },
          "correctedData": {
            "$ref": "#/components/schemas/ExtendedDataPointBigInteger"
          }
        }
      },
      "QaReportDataPointExtendedDataPointYesNoNoEvidenceFound": {
        "required": [
          "comment",
          "correctedData",
          "verdict"
        ],
        "type": "object",
        "properties": {
          "comment": {
            "type": "string"
          },
          "verdict": {
            "$ref": "#/components/schemas/QaReportDataPointVerdict"
          },
          "correctedData": {
            "$ref": "#/components/schemas/ExtendedDataPointYesNoNoEvidenceFound"
          }
        }
      },
      "QaReportDataPointMapStringCompanyReport": {
        "required": [
          "comment",
          "correctedData",
          "verdict"
        ],
        "type": "object",
        "properties": {
          "comment": {
            "type": "string"
          },
          "verdict": {
            "$ref": "#/components/schemas/QaReportDataPointVerdict"
          },
          "correctedData": {
            "type": "object",
            "additionalProperties": {
              "$ref": "#/components/schemas/CompanyReport"
            }
          }
        }
      },
      "QaReportDataPointVerdict": {
        "type": "string",
