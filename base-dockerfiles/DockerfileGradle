FROM eclipse-temurin:21.0.5_11-jdk-jammy@sha256:021c3f5ab987cc52ec7f76e332e2e0c621bc4bf9376eaeff486c12ee9fb83acc AS builder

WORKDIR /app

COPY ./gradle/ ./gradle/
COPY ./gradlew .
RUN  ./gradlew --version
COPY ./build.gradle.kts ./gradle.properties ./settings.gradle.kts ./versions.properties ./
RUN ./gradlew --no-daemon --stacktrace
