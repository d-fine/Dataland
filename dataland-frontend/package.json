{
  "name": "dataland-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lintci": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --max-warnings 0 --no-fix",
    "cypress": "cypress open",
    "checkfrontendcompilation": "tsc --noEmit -P tsconfig.app.json",
    "checkcypresscompilation": "tsc --noEmit -P tsconfig.cypress-e2e.json && tsc --noEmit -P tsconfig.cypress-ct.json",
    "testcomponent": "cypress run --component",
    "testpipeline": "cypress run --spec tests/e2e/specs/index.ts",
    "testgithubenvironment": "cypress run --config \"{\\\"e2e\\\": {\\\"baseUrl\\\": \\\"$npm_config_url\\\"}}\"",
    "fakefixtures": "ts-node -P tsconfig.commonjs.json -r tsconfig-paths/register tests/e2e/fixtures/GenerateFakeFixtures.ts",
    "checkfakefixturecompilation": "tsc --noEmit -P tsconfig.commonjs.json",
    "prepopulate": "cypress run --spec tests/e2e/specs/prepopulation/index.ts",
    "updatepackages": "npm-check-updates -u",
    "updatepackagelock": "npm update --save --include=dev && npm audit fix",
    "checkdependencies": "depcheck --ignores='@clients/*,@e2e/*,@sharedUtils/*,@ct/*,@vue/tsconfig,axios,tsconfig-paths,eslint-plugin-*,@vue/eslint-config-*'"
  },
  "dependencies": {
    "@formkit/core": "^1.2.2",
    "@formkit/themes": "^1.2.2",
    "@formkit/vue": "^1.2.2",
    "@ibm/plex": "^6.3.0",
    "axios": "^1.6.2",
    "currency-codes": "^2.1.0",
    "i18n-iso-countries": "^7.7.0",
    "keycloak-js": "^22.0.5",
    "material-icons": "^1.13.12",
    "pinia": "^2.1.7",
    "pinia-shared-state": "^0.4.5",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primevue": "~3.42.0",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.9",
    "@faker-js/faker": "^8.2.0",
    "@rushstack/eslint-patch": "^1.5.1",
    "@types/node": "20.8.6",
    "@vitejs/plugin-vue": "^4.4.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "12.11.0",
    "depcheck": "^1.4.7",
    "eslint": "^8.53.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "git-commit-id": "^2.0.1",
    "lint-staged": "^15.0.2",
    "mocha": "^10.2.0",
    "npm-check-updates": "^16.14.6",
    "otplib": "^12.0.1",
    "sass": "^1.69.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-istanbul": "^5.0.0"
  },
  "prettier": {
    "printWidth": 120
  },
  "lint-staged": {
    "*.vue": "eslint --fix",
    "*.ts": "eslint --fix"
  }
}
