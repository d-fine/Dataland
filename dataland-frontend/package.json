{
  "name": "dataland-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lintci": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --max-warnings 0 --no-fix",
    "cypress": "cypress open",
    "checkcypresscompilation": "tsc --noEmit -P tsconfig.cypress-e2e.json && tsc --noEmit -P tsconfig.cypress-ct.json",
    "testcomponent": "cypress run --component",
    "testpipeline": "cypress run",
    "testdevelopment": "cypress run --config '{\"e2e\": {\"baseUrl\": \"https://dev-dataland.duckdns.org\"}}'",
    "testpreview": "cypress run --config '{\"e2e\": {\"baseUrl\": \"https://dataland.com\"}}'",
    "fakefixtures": "ts-node -P tsconfig.commonjs.json -r tsconfig-paths/register tests/e2e/fixtures/GenerateFakeFixtures.ts",
    "updatepackages": "npm-check-updates -u",
    "updatepackagelock": "npm update --save --include=dev && npm audit fix",
    "checkdependencies": "depcheck --ignores='@clients/*,@e2e/*,@vue/tsconfig,axios,tsconfig-paths'"
  },
  "dependencies": {
    "@formkit/themes": "^1.0.0-beta.10",
    "@formkit/vue": "^1.0.0-beta.10",
    "@ibm/plex": "^6.1.0",
    "axios": "^0.27.2",
    "json2csv": "^5.0.7",
    "keycloak-js": "^19.0.2",
    "material-icons": "^1.11.11",
    "primeflex": "^3.2.1",
    "primeicons": "^5.0.0",
    "primevue": "~3.15.0",
    "vue": "^3.2.39",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@faker-js/faker": "^7.5.0",
    "@types/node": "^18.7.18",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/eslint-config-typescript": "^11.0.1",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^10.8.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.23.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.4.0",
    "git-commit-id": "^2.0.1",
    "mocha": "^10.0.0",
    "npm-check-updates": "^16.1.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.0",
    "vite-plugin-istanbul": "^3.0.1"
  },
  "prettier": {
    "printWidth": 120
  }
}
