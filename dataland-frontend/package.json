{
  "name": "dataland-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 8090",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "teste2e": "vue-cli-service test:e2e --mode development --url http://localhost:8090",
    "testheadless": "vue-cli-service test:e2e --mode development --url http://localhost:8090 --headless",
    "testpipeline": "vue-cli-service test:e2e --url http://proxy --headless",
    "testpreview": "vue-cli-service test:e2e --url http://preview-dataland.duckdns.org --headless",
    "testdevelopment": "vue-cli-service test:e2e --url http://dev-dataland.duckdns.org --headless",
    "testremote": "vue-cli-service test:e2e --url http://localhost",
    "testunit": "vue-cli-service test:unit",
    "fakefixtures": "ts-node -P tsconfig.commonjs.json tests/e2e/fixtures/fakeFixtures.ts",
    "updatepackages": "npm-check-updates -u",
    "updatepackagelock": "npm update --save --dev && npm audit fix"
  },
  "dependencies": {
    "@formkit/themes": "^1.0.0-beta.6",
    "@formkit/vue": "^1.0.0-beta.6",
    "axios": "^0.27.2",
    "axios-mock-adapter": "^1.20.0",
    "core-js": "^3.22.4",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^5.1.0",
    "get-parameter-names": "^0.3.0",
    "json2csv": "^5.0.7",
    "material-icons": "^1.10.9",
    "primeflex": "^3.1.3",
    "primeicons": "^5.0.0",
    "primevue": "^3.12.6",
    "vue": "^3.2.33",
    "vue-axios": "^3.4.1",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.0.15",
    "vue3-live-form": "^0.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.17.0",
    "@cypress/code-coverage": "^3.9.12",
    "@faker-js/faker": "^6.3.1",
    "@types/jest": "^27.5.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vue/cli-plugin-babel": "~5.0.4",
    "@vue/cli-plugin-e2e-cypress": "~5.0.4",
    "@vue/cli-plugin-eslint": "~5.0.4",
    "@vue/cli-plugin-typescript": "~5.0.4",
    "@vue/cli-plugin-unit-jest": "~5.0.4",
    "@vue/cli-service": "~5.0.4",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.21",
    "@vue/vue3-jest": "^27.0.0",
    "babel-jest": "^27.5.1",
    "cypress": "^9.6.0",
    "eslint": "^8.14.0",
    "eslint-plugin-vue": "^8.7.1",
    "git-commit-id": "^2.0.1",
    "jest": "^27.5.1",
    "node-sass": "^7.0.1",
    "sass-loader": "^12.6.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "~4.6.4",
    "npm-check-updates": "^12.5.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "nyc": {
    "report-dir": "coverage/e2e",
    "reporter": [
      "lcov",
      "json"
    ],
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.test.js",
      "**/test.js",
      "**/*.stories.js",
      "**/stories.js",
      "src/main.ts",
      "src/components/helper/*"
    ]
  }
}
