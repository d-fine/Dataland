{
  "name": "dataland-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lintci": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --max-warnings 0 --no-fix",
    "cypress": "cypress open",
    "checkcypresscompilation": "tsc --noEmit -P tsconfig.cypress-e2e.json && tsc --noEmit -P tsconfig.cypress-ct.json",
    "testcomponent": "cypress run --component",
    "testpipeline": "cypress run",
    "testgithubenvironment": "cypress run --config \"{\\\"e2e\\\": {\\\"baseUrl\\\": \\\"$npm_config_url\\\"}}\"",
    "fakefixtures": "ts-node -P tsconfig.commonjs.json -r tsconfig-paths/register tests/e2e/fixtures/GenerateFakeFixtures.ts",
    "checkfakefixturecompilation": "tsc --noEmit -P tsconfig.commonjs.json",
    "updatepackages": "npm-check-updates -u",
    "updatepackagelock": "npm update --save --include=dev && npm audit fix",
    "checkdependencies": "depcheck --ignores='@clients/*,@e2e/*,@sharedUtils/*,@ct/*,@vue/tsconfig,axios,tsconfig-paths,eslint-plugin-*,@vue/eslint-config-*'"
  },
  "dependencies": {
    "@formkit/core": "^1.0.0-beta.15",
    "@formkit/themes": "^1.0.0-beta.15",
    "@formkit/vue": "^1.0.0-beta.15",
    "@ibm/plex": "^6.2.0",
    "axios": "^1.3.5",
    "i18n-iso-countries": "^7.6.0",
    "json2csv": "^5.0.7",
    "keycloak-js": "^21.0.2",
    "material-icons": "^1.13.4",
    "pinia": "^2.0.34",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primevue": "~3.26.1",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@faker-js/faker": "^7.6.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/big.js": "^6.1.6",
    "@types/json2csv": "^5.0.3",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.1.3",
    "big.js": "^6.2.1",
    "cypress": "^12.9.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.38.0",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-jsdoc": "^41.1.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.11.0",
    "git-commit-id": "^2.0.1",
    "lint-staged": "^13.2.1",
    "mocha": "^10.2.0",
    "npm-check-updates": "^16.10.8",
    "otplib": "^12.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vite-plugin-istanbul": "^4.0.1"
  },
  "prettier": {
    "printWidth": 120
  },
  "lint-staged": {
    "*.vue": "eslint --fix",
    "*.ts": "eslint --fix"
  }
}
