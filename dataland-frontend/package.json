{
  "name": "dataland-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lintci": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --max-warnings 0 --no-fix",
    "cypress": "cypress open",
    "checkcypresscompilation": "tsc --noEmit -P tsconfig.cypress-e2e.json && tsc --noEmit -P tsconfig.cypress-ct.json",
    "testcomponent": "cypress run --spec tests/component/components/resources/frameworkDataSearch/sme/SmePanel.cy.ts --component",
    "testpipeline": "cypress run --spec tests/e2e/specs/index.ts",
    "testgithubenvironment": "cypress run --config \"{\\\"e2e\\\": {\\\"baseUrl\\\": \\\"$npm_config_url\\\"}}\"",
    "fakefixtures": "ts-node -P tsconfig.commonjs.json -r tsconfig-paths/register tests/e2e/fixtures/GenerateFakeFixtures.ts",
    "checkfakefixturecompilation": "tsc --noEmit -P tsconfig.commonjs.json",
    "prepopulate": "cypress run --spec tests\\e2e\\specs\\prepopulation\\index.ts",
    "updatepackages": "npm-check-updates -u",
    "updatepackagelock": "npm update --save --include=dev && npm audit fix",
    "checkdependencies": "depcheck --ignores='@clients/*,@e2e/*,@sharedUtils/*,@ct/*,@vue/tsconfig,axios,tsconfig-paths,eslint-plugin-*,@vue/eslint-config-*'"
  },
  "dependencies": {
    "@formkit/core": "^0.17.5",
    "@formkit/themes": "^0.17.5",
    "@formkit/vue": "^0.17.5",
    "@ibm/plex": "^6.3.0",
    "axios": "^1.4.0",
    "currency-codes": "^2.1.0",
    "i18n-iso-countries": "^7.6.0",
    "keycloak-js": "^22.0.1",
    "material-icons": "^1.13.9",
    "pinia": "^2.1.6",
    "pinia-shared-state": "^0.4.5",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primevue": "~3.32.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.11.0",
    "@faker-js/faker": "^8.0.2",
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/node": "20.5.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "12.11.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.47.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-jsdoc": "^46.4.6",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "git-commit-id": "^2.0.1",
    "lint-staged": "^13.3.0",
    "mocha": "^10.2.0",
    "npm-check-updates": "^16.11.1",
    "otplib": "^12.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-istanbul": "^4.1.0"
  },
  "prettier": {
    "printWidth": 120
  },
  "lint-staged": {
    "*.vue": "eslint --fix",
    "*.ts": "eslint --fix"
  }
}
