{
  "name": "dataland-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lintci": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --max-warnings 0 --no-fix",
    "cypress": "cypress open",
    "checkcypresscompilation": "tsc --noEmit -P tsconfig.cypress-e2e.json && tsc --noEmit -P tsconfig.cypress-ct.json",
    "testcomponent": "cypress run --component --config video=false",
    "testpipeline": "cypress run",
    "testdevelopment": "cypress run --config '{\"e2e\": {\"baseUrl\": \"https://dev-dataland.duckdns.org\"}}'",
    "testpreview": "cypress run --config '{\"e2e\": {\"baseUrl\": \"https://dataland.com\"}}'",
    "fakefixtures": "ts-node -P tsconfig.commonjs.json -r tsconfig-paths/register tests/e2e/fixtures/GenerateFakeFixtures.ts",
    "updatepackages": "npm-check-updates -u",
    "updatepackagelock": "npm update --save --include=dev && npm audit fix",
    "checkdependencies": "depcheck --ignores='@clients/*,@e2e/*,@vue/tsconfig,axios,tsconfig-paths,eslint-plugin-*,@vue/eslint-config-*'"
  },
  "dependencies": {
    "@formkit/themes": "^1.0.0-beta.11",
    "@formkit/vue": "^1.0.0-beta.11",
    "@ibm/plex": "^6.1.1",
    "axios": "^1.1.3",
    "i18n-iso-countries": "^7.5.0",
    "json2csv": "^5.0.7",
    "keycloak-js": "^19.0.3",
    "material-icons": "^1.12.0",
    "primeflex": "^3.2.1",
    "primeicons": "^5.0.0",
    "primevue": "~3.15.0",
    "vue": "^3.2.39",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@faker-js/faker": "^7.6.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/big.js": "^6.1.6",
    "@types/node": "^18.11.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "big.js": "^6.2.1",
    "cypress": "^10.10.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.25.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "git-commit-id": "^2.0.1",
    "mocha": "^10.1.0",
    "npm-check-updates": "^16.3.13",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vite-plugin-istanbul": "^3.0.1"
  },
  "prettier": {
    "printWidth": 120
  }
}
